{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=fr",
    "start:fr": "ng serve --configuration=fr",
    "start:en": "ng serve --configuration=en",
    "build": "ng build --configuration=fr",
    "build:fr": "ng build --configuration=fr",
    "build:en": "ng build --configuration=en",
    "build:i18n": "ng --prod --aot --optimization --vendor-chunk --common-chunk --extract-licenses --extract-css --localize build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "xi18n": "bash -c 'for l in fr en; do ng xi18n --output-path locale --out-file _messages.${l}.xlf --i18n-locale ${l}; done'",
    "brotli": "bash -c 'for f in ~/citizen/frontend/dist/browser/*/*.{css,js}; do brotli ${f}; done'",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "dev:ssr": "ng run frontend:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng run frontend:build:fr && ng run frontend:build:en && ng run frontend:server:production"
  },
  "private": true,
  "dependencies": {
