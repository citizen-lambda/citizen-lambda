    "font-awesome": "^4.7.0",
    "jquery": "^3.4.0",
    "jshint": "^2.9.6",
    "leaflet": "^1.5.1",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-gesture-handling": "^1.1.8",
    "leaflet.heat": "^0.2.0",
    "leaflet.locatecontrol": "^0.66.2",
