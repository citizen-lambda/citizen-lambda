{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "appData": {
    "version": "0.0.1",
    "changelog": "  â€¢ updating works"
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|JPG|jpeg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ],
        "urls": ["https://taxref.mnhn.fr/api/media/download/thumbnail/*"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "observations",
      "urls": ["/api/programs/*/observations"],
      "cacheConfig": {
        "maxSize": 25,
        "maxAge": "10m",
        "timeout": "30s",
        "strategy": "freshness"
      }
    },
    {
      "name": "programs",
      "urls": ["/api/programs", "/api/programs/*"],
      "cacheConfig": {
        "maxSize": 32,
        "maxAge": "10m",
        "timeout": "30s",
        "strategy": "performance"
      }
    },
    {
      "name": "taxonomy",
      "urls": ["/api/taxonomy/lists/*/species", "/api/taxref/*"],
      "cacheConfig": {
        "maxSize": 128,
        "maxAge": "10m",
        "timeout": "30s",
        "strategy": "performance"
      }
    }
  ]
}
